generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id        Int    @id @default(autoincrement())
  nome      String
  email     String @unique
  idade     Int
  matricula String

  @@map("alunos")
}

model Professor {
  id         Int    @id @default(autoincrement())
  nome       String
  email      String @unique
  idade      Int
  disciplina String

  @@map("professores")
}